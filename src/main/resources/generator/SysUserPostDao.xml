<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="generator.SysUserPostDao">
  <resultMap id="BaseResultMap" type="generator.SysUserPostKey">
    <id column="user_id" jdbcType="BIGINT" property="userId"/>
    <id column="post_id" jdbcType="BIGINT" property="postId"/>
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="generator.SysUserPostKey">
    delete
    from "sys_user_post"
    where user_id = #{userId,jdbcType=BIGINT}
      and post_id = #{postId,jdbcType=BIGINT}
  </delete>
  <insert id="insert" keyColumn="user_id" keyProperty="userId" parameterType="generator.SysUserPostKey" useGeneratedKeys="true">
    insert into "sys_user_post" (post_id)
    values (#{postId,jdbcType=BIGINT})
  </insert>
  <insert id="insertSelective" keyColumn="user_id" keyProperty="userId" parameterType="generator.SysUserPostKey" useGeneratedKeys="true">
    insert into "sys_user_post"
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="postId != null">
        post_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="postId != null">
        #{postId,jdbcType=BIGINT},
      </if>
    </trim>
  </insert>
</mapper>
